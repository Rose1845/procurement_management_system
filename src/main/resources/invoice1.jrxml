    <queryString>
        <![CDATA[SELECT
        i.*,
        po.*,
        it.*,
        s.name
        s.address.box AS supplier_address_box,
        s.address.country AS supplier_address_country,
        s.address.city AS supplier_address_city,
        s.address.location AS supplier_address_location,
    FROM
        Invoice i
    JOIN
        PurchaseOrder po ON i.purchaseOrderId = po.purchaseOrderId
    JOIN
        OrderItems oi ON po.purchaseOrderId = oi.purchaseOrderId
    JOIN
        Item it ON oi.itemId = it.itemId
    JOIN
        Supplier s ON po.supplierId = s.vendorId
    WHERE
        i.invoiceId = $P{invoiceId}]]>
    </queryString>

