    <parameter name="purchaseOrderId" class="java.lang.Long"/>
    <queryString>
        <![CDATA[SELECT
    po.*,
    i.item_name,
    i.quantity,
    i.total_price,
    i.item_number,
    i.unit_price,
    s.name AS supplier_name,
    s.p_o_box AS supplier_address_box,
    s.country AS supplier_address_country,
    s.city AS supplier_address_city,
    s.location AS supplier_address_location,
    CONCAT(u.firstname, ' ', u.lastname) AS created_by_name,
    CASE po.approval_status
        WHEN 0 THEN 'PENDING'
        WHEN 1 THEN 'APPROVED'
        WHEN 2 THEN 'REJECT'
        WHEN 3 THEN 'ISSUED'
        WHEN 4 THEN 'IN_DELIVERY'
        WHEN 5 THEN 'FULLY_RECEIVED'
        WHEN 6 THEN 'CLOSED'
        ELSE 'UNKNOWN'
    END AS approval_status_name
FROM
    purchase_order po
JOIN
    order_items oi ON po.purchase_order_id = oi.purchase_order_id
JOIN
    item i ON oi.item_id = i.item_id
JOIN
    supplier s ON po.supplier_id = s.vendor_id
JOIN
    user u ON po.created_by = u.id
WHERE
    po.purchase_order_id = $P{purchaseOrderId};
]]>
    </queryString>
