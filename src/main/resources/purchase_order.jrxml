    <parameter name="purchaseOrderId" class="java.lang.Long"/>
    <queryString>
        <![CDATA[SELECT
            po.purchaseOrderId,
            po.totalAmount,
            po.approvalStatus,
            po.createdAt,
            po.createdBy,
            po.deliveryDate,
            po.paymentType,
            po.purchaseOrderTitle,
            po.termsAndConditions,
            po.updatedAt,
            s.supplierName AS supplier_name,
            s.pOBox AS supplier_address_box,
            s.country AS supplier_address_country,
            s.city AS supplier_address_city,
            s.location AS supplier_address_location,
            i.itemName,
            i.quantity,
            i.totalPrice,
            i.itemNumber,
            i.unitPrice
        FROM PurchaseOrder po
        JOIN order_items oi ON po.purchaseOrderId = oi.purchaseOrderId
        JOIN item i ON oi.item_id = i.item_id
        JOIN supplier s ON po.supplier_id = s.vendor_id
        WHERE po.purchaseOrderId = $P{purchaseOrderId}]]>
    </queryString>
